{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop":{
    "children":[
      "flex-layout.row#footer-desktop"
    ]
  },
  "footer-layout.mobile":{
    "children":[
      "rich-text#footer-text-mobile"
    ]
  },
  "flex-layout.row#footer-desktop": {
    "children": [
      "rich-text#footer-text"
    ]
  },
  "rich-text#footer-text":{
    "props":{
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "Todos los derechos reservados 2020 Multifarmacias de descuento®"
    }
  },
  "rich-text#footer-text-mobile":{
    "props":{
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "Todos los derechos reservados 2020 Multifarmacias de descuento®",
      "font-size": "12px"
    }
  }
}
