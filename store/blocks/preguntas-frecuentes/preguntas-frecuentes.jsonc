	"rich-text#response10":{
		"props": {
		    "textAlignment": "LEFT",
		    "textPosition": "LEFT",
		    "text": "En pedido con monto mayor a $100.00 pesos No tiene costo. En caso de ser menor a $100 pesos el envío costará $30.00 pesos", 
		    "textColor": "c-on-emphasis",
		    "font": "t-heading-5",
		    "blockClass": "rich-response"
		 }
	},

	"rich-text#question11":{
		"props": {
		    "textAlignment": "LEFT",
		    "textPosition": "LEFT",
		    "text": "- ¿Cuál es el horario del Servicio a Domicilio?",
		    "textColor": "c-on-emphasis",
		    "font": "t-heading-5",
