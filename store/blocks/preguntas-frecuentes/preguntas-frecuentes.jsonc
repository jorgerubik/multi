	"rich-text#question18":{
		"props": {
		    "textAlignment": "LEFT",
		    "textPosition": "LEFT",
		    "text": "- ¿Cómo cancelo, modifico o devuelvo mi pedido?",
		    "textColor": "c-on-emphasis",
